{
  "name": "dothis-projects",
  "version": "0.0.0",
  "private": true,
  "main": "./prisma/client.ts",
  "packageManager": "pnpm@7.15.0",
  "scripts": {
    "build": "dotenv -e .env -- turbo run build",
    "dev": "dotenv -e .env -- turbo run dev --parallel",
    "clean:workspaces": "turbo clean",
    "clean": "rm -rf node_modules",
    "lint": "turbo run lint",
    "studio": "turbo run studio",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db-pull": "turbo db-pull --parallel",
    "db-push": "turbo db-push --parallel",
    "db-generate": "dotenv -e .env -- turbo db-generate --parallel",
    "theme-generate": "turbo theme-generate --parallel",
    "debug-dev": "turbo debug-dev"
  },
  "devDependencies": {
    "@dothis/config": "*",
    "prettier": "^2.8.0",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^1.8.0",
    "@anatine/zod-openapi": "^1.10.0",
    "dotenv-cli": "^6.0.0",
    "nestjs-zod": "^1.2.1",
    "openapi3-ts": "^3.1.2",
    "zod": "^3.19.1"
  }
}
