{
  "name": "dothis-projects",
  "version": "0.0.0",
  "private": true,
  "main": "./prisma/client.ts",
  "packageManager": "pnpm@7.15.0",
  "scripts": {
    "build": "dotenv -e .env -- turbo run build",
    "dev": "dotenv -e .env -- turbo run dev --parallel",
    "dev:request": "dotenv -e .env -- turbo run dev --filter=request-web",
    "dev:dashboard": "dotenv -e .env -- turbo run dev --filter=dashboard-web",
    "storybook:share": "turbo run storybook --scope='@dothis/share'",
    "build-storybook:share": "turbo run build-storybook --scope='@dothis/share'",
    "clean:workspaces": "turbo clean",
    "clean": "rm -rf node_modules",
    "lint": "turbo run lint",
    "studio": "turbo run studio",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db-pull": "turbo db-pull --parallel",
    "db-push": "turbo db-push --parallel",
    "db-gen": "dotenv -e .env -- turbo db-generate --parallel",
    "theme-gen": "turbo theme-gen --parallel",
    "debug-dev": "turbo debug-dev"
  },
  "devDependencies": {
    "@dothis/config": "*",
    "prettier": "^2.8.0",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "dotenv-cli": "^6.0.0"
  },
  "pnpm": {
    "overrides": {
      "got@<11.8.5": ">=11.8.5",
      "d3-color@<3.1.0": ">=3.1.0",
      "trim@<0.0.3": ">=0.0.3",
      "trim-newlines@<3.0.1": ">=3.0.1",
      "glob-parent@<5.1.2": ">=5.1.2"
    }
  }
}
