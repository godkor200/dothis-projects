{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^db-generate", "^theme-generate", "^build"],
      "outputs": [".next/**"]
    },
    "theme-generate": {
      "inputs": ["./lib/styles/**/*"],
      "outputs": ["./lib/types/theme.d.ts"],
      "cache": true
    },
    "db-generate": {
      "inputs": ["prisma/schema.prisma"],
      "cache": true
    },
    "db-push": {
      "inputs": ["prisma/schema.prisma"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^db-generate", "^theme-generate"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "request-web": {
      "dependsOn": ["^build"],
      "env": [
        "DATABASE_URL",
        "NEXTAUTH_URL",
        "NEXTAUTH_SECRET",
        "AWS_ACCESS_KEY_ID",
        "AWS_SECRET_ACCESS_KEY"
      ]
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "NEXTAUTH_URL",
    "NEXTAUTH_SECRET",
    "AWS_ACCESS_KEY_ID",
    "AWS_SECRET_ACCESS_KEY"
  ]
}
