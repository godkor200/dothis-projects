{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=development nest build --webpack && tsc-alias -p tsconfig.build.json",
    "build:api": "nest build --webpack --webpackPath ./apps/api/webpack.config.js && tsc-alias -p ./apps/api/tsconfig.app.json",
    "build:external-sales-api": "nest build --webpack --webpackPath ./apps/external-sales-api/webpack.config.js && tsc-alias -p ./apps/external-sales-api/tsconfig.app.json",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev-api": "cross-env NODE_ENV=development nest build api --webpack --webpackPath ./apps/api/webpack.config.js --watch",
    "start:prod-api": "cross-env NODE_ENV=production nest build api --webpack --webpackPath ./apps/api/webpack.config.js --watch",
    "start:debug-api": "cross-env NODE_ENV=development nest build api --webpack --webpackPath ./apps/api/webpack.config.js --debug --watch",
    "start:dev-external-sales-api": "cross-env NODE_ENV=development nest build external-sales-api --webpack --webpackPath ./apps/external-sales-api/webpack.config.js --watch",
    "start:prod-external-sales-api": "cross-env NODE_ENV=production nest build external-sales-api --webpack --webpackPath ./apps/external-sales-api/webpack.config.js --watch",
    "start:debug-external-sales-api": "cross-env NODE_ENV=development nest build external-sales-api --webpack --webpackPath ./apps/external-sales-api/webpack.config.js --debug --watch",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "typeorm": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm/cli.js",
    "typeorm-prod": "cross-env NODE_ENV=production ts-node ./node_modules/typeorm/cli.js",
    "typeorm-create": "npm run typeorm -- migration:create apps/api/src/migrations/migration",
    "typeorm-run": "npm run typeorm migration:run -- -d apps/api/src/typeorm.config.ts",
    "typeorm-generate": "npm run typeorm -- -d apps/api/src/typeorm.config.ts migration:generate apps/api/src/migrations/migration",
    "typeorm-revert": "npm run typeorm -- -d apps/api/src/typeorm.config.ts migration:revert",
    "typeorm-create:prod": "npm run typeorm-prod -- migration:create apps/api/src/migrations/migration",
    "typeorm-run:prod": "npm run typeorm-prod migration:run -- -d apps/api/src/typeorm.config.ts",
    "typeorm-generate:prod": "npm run typeorm-prod -- -d apps/api/src/typeorm.config.ts migration:generate apps/api/src/migrations/migration",
    "typeorm-revert:prod": "npm run typeorm-prod -- -d apps/api/src/typeorm.config.ts migration:revert",


    "typeorm-create:ex-api": "npm run typeorm -- migration:create apps/external-sales-api/src/migrations/migration",
    "typeorm-run:ex-api": "npm run typeorm migration:run -- -d apps/external-sales-api/src/typeorm.config.ts",
    "typeorm-generate:ex-api": "npm run typeorm -- -d apps/external-sales-api/src/typeorm.config.ts migration:generate apps/external-sales-api/src/migrations/migration",
    "typeorm-revert:ex-api": "npm run typeorm -- -d apps/external-sales-api/src/typeorm.config.ts migration:revert"
  },
