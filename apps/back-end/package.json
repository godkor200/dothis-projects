{
  "name": "back-end",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider cross-env NODE_ENV=development nest build --webpack && tsc-alias -p tsconfig.build.json",
    "clean": "rm -rf node_modules",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development cross-env NODE_OPTIONS=--openssl-legacy-provider nest build --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/back-end/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./libs/commons/src/migrations/dataSource.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./libs/commons/src/migrations/dataSource.ts migration:generate libs/commons/src/migrations/generate",
    "typeorm:create-migration": "npm run typeorm -- migration:create libs/commons/src/migrations/migrations",
    "typeorm:revert-migration": "npm ts-typeorm migration:revert"
  },
  "dependencies": {
    "@abitia/zod-dto": "^0.4.0",
    "@anatine/zod-nestjs": "^1.9.2",
    "@nestjs/common": "^9.3.8",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.8",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@ts-rest/core": "^3.17.0",
    "@ts-rest/nest": "^3.17.0",
    "@ts-rest/open-api": "^3.17.0",
    "axios": "^1.2.2",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "googleapis": "^107.0.0",
    "joi": "^17.7.1",
    "mysql2": "^2.3.3",
    "nestjs-zod": "^1.2.1",
    "openapi3-ts": "^3.1.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "4.1.2",
    "typeorm": "^0.3.12",
    "uuid": "^9.0.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "^16.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/reflect-metadata": "^0.1.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.0.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.4.1",
    "prettier": "^2.8.0",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^5.3.6",
    "typescript": "^4.7.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  }
