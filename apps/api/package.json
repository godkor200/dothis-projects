{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=development nest build --webpack && tsc-alias -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js",
    "start:dev-api": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js --watch",
    "start:prod-api": "cross-env NODE_ENV=production nest build --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "cross-env NODE_ENV=development nest build --webpack --webpackPath webpack.config.js --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "^1.9.2",
    "@anatine/zod-openapi": "^1.12.0",
    "@aws-sdk/client-athena": "^3.312.0",
    "@dothis/dto": "workspace:*",
    "@googleapis/youtube": "^8.0.2",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "9.0.1",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "9.0.1",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "9.0.1",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@ts-rest/core": "^3.18.0",
    "@ts-rest/nest": "^3.18.0",
    "@ts-rest/open-api": "^3.18.1",
    "@types/webpack-env": "^1.18.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "clean-webpack-plugin": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
