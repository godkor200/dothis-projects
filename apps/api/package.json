    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "run-script-webpack-plugin": "^0.1.1",
    "rxjs": "7.5.5",
    "terser-webpack-plugin": "^5.3.6",
    "tsc-alias": "^1.8.2",
    "typeorm": "^0.3.12",
